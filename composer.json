{
    "name": "json-mini-bread",
    "description": "Creates a mini BREAD nested inside another model.",
    "require": {
        "larapack/hooks": "^1.0.5",
        "tcg/voyager": "^1.2.7"
    },
    "autoload": {
        "psr-4": {
            "JsonMiniBreadHook\\": "src/"
        }
    },
    "extra": {
        "hook": {
            "providers": [
                "JsonMiniBreadHook\\JsonMiniBreadHookServiceProvider"
            ],
            "aliases": {
                "JsonMiniBreadHook": "JsonMiniBreadHook\\Facades\\JsonMiniBreadHookFacade"
            },
            "views": [
                "resources/views"
            ],
            "lang": [
                "resources/lang"
            ]
        }
    }
}
